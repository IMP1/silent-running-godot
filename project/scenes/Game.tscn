code = "shader_type canvas_item;

uniform vec4 ping_colour : hint_color = vec4(1.0) ;

const float EMPTY = 32.0 / 255.0;

void fragment() {
	float solid = 0.0;
	float edge = 0.0;
	vec4 background = textureLod(SCREEN_TEXTURE, SCREEN_UV, 0.0);
	if (background.r > EMPTY) {
		solid = 1.0;
	}
	vec4 neighbour_top = textureLod(SCREEN_TEXTURE, SCREEN_UV + vec2(0, -TEXTURE_PIXEL_SIZE.y), 0.0);
	vec4 neighbour_left = textureLod(SCREEN_TEXTURE, SCREEN_UV + vec2(-TEXTURE_PIXEL_SIZE.x, 0), 0.0);
	vec4 neighbour_right = textureLod(SCREEN_TEXTURE, SCREEN_UV + vec2(TEXTURE_PIXEL_SIZE.x, 0), 0.0);
	vec4 neighbour_bottom = textureLod(SCREEN_TEXTURE, SCREEN_UV + vec2(0, TEXTURE_PIXEL_SIZE.y), 0.0);
	if (neighbour_top.r <= EMPTY && background.r > EMPTY) {
		edge += 1.0;
	}
	if (solid > 0.0 && edge > 0.0) {
		COLOR = ping_colour;
	} else {
		discard;
	}
}"

